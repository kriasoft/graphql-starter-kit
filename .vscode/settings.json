{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "files.associations": {
    ".env.**": "dotenv"
  },
  "search.exclude": {
    "**/lib/": true,
    "yarn-error.log": true,
    "yarn.lock": true
  },
  "cSpell.ignoreWords": [
    "abcdefghijklmnopqrstuvwxyz",
    "cloudsql",
    "corejs",
    "dataloader",
    "datname",
    "envalid",
    "jsonb",
    "knexfile",
    "kriasoft",
    "pgappname",
    "pgdatabase",
    "pgdebug",
    "pghost",
    "pgpassword",
    "pgport",
    "pgsslcert",
    "pgsslkey",
    "pgsslmode",
    "pgsslrootcert",
    "pguser",
    "pnpify",
    "postgres",
    "psql",
    "sendgrid"
  ]
}
