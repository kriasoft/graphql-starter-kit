{
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "jest.jestCommandLine": "yarn jest",
  "jestrunner.enableYarnPnpSupport": true,
  "jestrunner.yarnPnpCommand": "yarn-4.0.0-rc.6.cjs",
  "files.associations": {
    ".terraformrc": "terraform"
  },
  "files.exclude": {
    ".cache": true,
    ".editorconfig": true,
    ".eslintcache": true,
    ".git": true,
    ".gitattributes": true,
    ".gitignore": true,
    ".husky": true,
    ".pnp.cjs": true,
    ".pnp.loader.mjs": true,
    ".prettierignore": true,
    ".yarn": true,
    ".yarnrc.yml": true,
    "LICENSE": true,
    "yarn.lock": true
  },
  "search.exclude": {
    "**/dist/": true,
    "**/yarn-error.log": true,
    "**/yarn.lock": true,
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "cSpell.ignoreWords": [
    "abcdefghijklmnopqrstuvwxyz",
    "browserslist",
    "citext",
    "cloudfunctions",
    "cloudsql",
    "corejs",
    "dataloader",
    "datname",
    "devtool",
    "downlevel",
    "entrypoint",
    "envalid",
    "envars",
    "eslintcache",
    "esmodules",
    "esnext",
    "execa",
    "firestore",
    "gamecenter",
    "gcloud",
    "globby",
    "GraphQLID",
    "gsutil",
    "gtag",
    "jsonb",
    "knexfile",
    "kriasoft",
    "linkedin",
    "miniflare",
    "nothrow",
    "octokit",
    "pathinfo",
    "pgappname",
    "pgdatabase",
    "pgdebug",
    "pghost",
    "pgpassword",
    "pgport",
    "pgservername",
    "pgsslcert",
    "pgsslkey",
    "pgsslmode",
    "pgsslrootcert",
    "pguser",
    "playgames",
    "pnpify",
    "postgres",
    "psql",
    "refetchable",
    "sendgrid",
    "sourcemap",
    "spdx",
    "tfvars",
    "timestamptz",
    "tslib",
    "typechecking",
    "unprocessable",
    "upsert",
    "webflow",
    "yarnpkg",
    "yarnrc"
  ],
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  }
}
